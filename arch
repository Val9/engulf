control <-> node <-> worker <-> runner
aggregator <-> collector


# Type Job:
# Job:
{id: UUID(str)
 url: http://.....com
 state: (starting:running:stopped)
 started-at: Timestamp
 ended-at: Timestamp
}

# Type Node:
{id: UUID(str)
 hostname: Str
 state: (idle:running)}


(ns control)

(def start [job]
  (doseq [node nodes] (send msg :start job)))

(def stop []
  (doseq [node nodes] (send-msg node :stop)))



# Control
{:id UUID(str)
 :state
 :hostname HOSTNAME
 :current-job}

# Node
{:id UUID(str)
 :state
 :hostname HOSTNAME
 :worker-count 20
 :current-job}

